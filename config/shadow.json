{
    "name": "depth_estimation_test1",
    "phase": "train",
    "gpu_ids": [
       0
    ],
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": null
    },
    "datasets": {
        "train": {
            "name": "DepthTrainSet",
            "mode": "depth",
            "dataroot": "D:/FYP-001/MS2dataset",
            "datatype": "img",
            "batch_size": 8,
            "num_workers": 4,
            "use_shuffle": true,
            "data_len": -1
        },
        "val": {
            "name": "DepthValSet",
            "mode": "depth",
            "dataroot": "D:/FYP-001/MS2dataset",
            "datatype": "img",
            "data_len": -1
        }
    },
    "model": {
        "which_model_G": "sr3",
        "finetune_norm": false,
        "unet": {
            "in_channel": 2,
            "out_channel": 1,
            "inner_channel": 64,
            "norm_groups": 16,
            "channel_multiplier": [
                1,
                2,
                4,
                8
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0
        },
        "beta_schedule": {
            "train": {
                "schedule": "linear",
                "n_timestep": 500,
                "linear_start": 1e-4,
                "linear_end": 0.02
            },
            "val": {
                "schedule": "linear",
                "n_timestep": 500,
                "linear_start": 1e-4,
                "linear_end": 0.02
            }
        },
        "diffusion": {
            "image_size": 256,
            "channels": 1,
            "conditional": true
        }
    },
    "train": {
        "n_iter": 700000,
        "val_freq": 1000,
        "save_checkpoint_freq": 5000,
        "print_freq": 200,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        },
        "ema_scheduler": {
            "step_start_ema": 5000,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "depth_estimation_project"
    },
    "distributed": false
}
